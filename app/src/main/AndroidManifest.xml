<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lenovo.myoucapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UploadActivity"></activity>
        <activity android:name=".MyItemActivity" />
        <activity android:name=".EnterShopActivity" />
        <activity android:name=".CollectionActivity" />
        <activity android:name=".ResultActivity" />
        <activity android:name=".SearchActivity" />

        <meta-data
            android:name="com.google.android.actions"
            android:resource="@xml/first_actions" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>